#include <stdio.h>

int gcd(int a,int b){ return b==0?a:gcd(b,a%b); }

int main(){
    char ciph[100]="BU...";
    int a,b,i;
    for(a=1;a<26;a++){
        if(gcd(a,26)!=1) continue;
        for(b=0;b<26;b++){
            printf("\na=%d b=%d : ",a,b);
            for(i=0;ciph[i];i++){
                int c=ciph[i]-'A';
                int p=(a*(c-b+26))%26;
                printf("%c",p+'A');
            }
        }
    }
}
