#include <stdio.h>
#include <ctype.h>

void decrypt(char *c,int k){
    for(int i=0;c[i];i++){
        if(isalpha(c[i]))
            printf("%c",(c[i]-'A'-k+26)%26+'A');
        else printf("%c",c[i]);
    }
    printf("\n");
}

int main(){
    char ciph[100];
    gets(ciph);

    for(int k=0;k<26;k++){
        printf("Key %d: ",k);
        decrypt(ciph,k);
    }
}
